# QualityGate環境設定
# GitHub token - set in your environment or .env.local
# GITHUB_TOKEN=your_token_here

# QualityGate設定
QUALITYGATE_MODE=production
QUALITYGATE_PHASE=4
HFP_ARCHITECTURE_PHASE=5

# Database設定
DATABASE_URL=sqlite:///./qualitygate.db

# AI Learning Edition設定
AI_LEARNING_PHASE=3A_Week_4B
PATTERN_MEMORY_ENABLED=true
PERFORMANCE_MONITORING=true

# Security設定
OWASP_COMPLIANCE=enabled
SECURITY_AUDIT=daily